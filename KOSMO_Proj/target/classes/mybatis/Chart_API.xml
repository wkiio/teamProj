<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper  
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace속성 : 매퍼파일의 완전한 경로 .xml은 생략 -->
<!-- ★ibatis와는 다르게 id값에 .(dot)를 사용할 수 없다. -->

<!-- ==================================================================== -->
<!-- 		컬럼 순서는 NO,TITLE,THUMBNAIL,URL 입니다						  -->
<!-- 		컬럼 순서에 유의하며 SQL문을 작성해주세요 								  -->
<!-- ==================================================================== -->


<mapper namespace="mybatis.Chart">
	<!-- 요기 안에다 쿼리문 작성하시면 됩니다 -->
	
	<select id="SelectList" parameterType="java.util.Map"  resultType="Chart_DTO">
		select * from Bar_Chart
	</select>
	
	<select id="MemberCount" parameterType="java.util.Map" resultType="int">
		select count(*) from members
	</select>
	
	<select id="CarpoolCount" parameterType="java.util.Map" resultType="int">
		select count(*) from members where PARTNERSTATUS = '2'
	</select>
	
	
	
	
</mapper>