<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper  
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mybatis.Partner">

	<select id="partnerOne" resultType="com.kosmo.baby.service.PartnerDTO" parameterType="java.util.Map">
		select p.id,cartype,carnumber,name from partner_members p JOIN members m ON p.id = m.id where p.id=#{id}
	</select>

</mapper>