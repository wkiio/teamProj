<script type="text/javascript">
$(function(){
	
	if(!window.closevar){
		$("#contents_warp").mouseover(function(){ 
			if(window.closevar != 0){
				$(".gnb07").trigger("mouseover");
			}
		});
	}
	
	
	
	if(!window.closevar){
		$("#contents_warp").mouseover(function(){  
			if(window.closevar != 0){
				$(".gnb07").trigger("mouseover");
			}
		}); 
	}
	
	
	showClosebtn($.map($(".gnb").find("a"),function(o,idx){
		if($(o).hasClass("on")){
			return (idx+1);
		}
	}));

	function allCloseMenu(){
		var check = false;
		$(".subMenu").each(function(){
			if($(this).css("display") != "none"){
				check = true;
			}
		});
		//$(".subMenu").css("display","none");
		return check;
	}

	function showClosebtn(objI){
		if($('.subMenu0'+objI).length > 0){
			var closeTop = $('.subMenu0'+objI).position().top+$('.subMenu0'+objI).height();		//닫기 버튼 높이값
//alert(closeTop);     
			if(closeTop <= 1){		//높이값 혹시 몰라서 저장해둠
				closeTop = {
					1: 119,
					2: 119,
					3: 119,
					4: 119,
					5: 119,
					6: 119,
					7: 159
